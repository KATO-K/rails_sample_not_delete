{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "source-bundle",
      "host": {
        "sourcePath": "/var/app/current"
      }
    },
    {
      "name": "public-data",
      "host": {
        "sourcePath": "/var/app/current/public"
      }
    },
    {
      "name": "tmp-data"
    }
  ],
  "containerDefinitions": [
    {
      "name": "webapp-app",
      "image": "017522386375.dkr.ecr.ap-northeast-1.amazonaws.com/tech_video:latest",
      "essential": true,
      "memory": 2000,
      "command": ["bundle","exec","puma","-C","config/puma.rb"],
      "mountPoints": [
        {
          "sourceVolume": "source-bundle",
          "containerPath": "/var/www/webapp"
        },
        {
          "sourceVolume": "tmp-data",
          "containerPath": "/var/www/webapp/tmp"
        },
        {
          "sourceVolume": "awseb-logs-webapp-app",
          "containerPath": "/var/www/webapp/log"
        }
      ]
    },
    {
      "name": "nginx",
      "image": "017522386375.dkr.ecr.ap-northeast-1.amazonaws.com/nginx:latest",
      "essential": true,
      "memory": 2000,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "webapp-app"
      ],
      "dependsOn": [
        {
          "containerName": "webapp-app",
          "condition": "START"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "tmp-data",
          "containerPath": "/var/www/webapp/tmp"
        },
        {
          "sourceVolume": "public-data",
          "containerPath": "/var/www/webapp/public"
        },
        {
          "sourceVolume": "awseb-logs-nginx",
          "containerPath": "/var/log/nginx"
        }
      ]
    }
  ]
}